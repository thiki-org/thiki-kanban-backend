<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="org.thiki.kanban.registration.RegistrationPersistence">

    <insert id="register" parameterType="org.thiki.kanban.registration.Registration">
        INSERT INTO
        kb_user_registration(id, password,name,email,salt)
        VALUES (#{id}, #{password},#{name}, #{email},#{salt})
    </insert>

</mapper>
